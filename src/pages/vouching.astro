---
import VouchingTable from "../components/VouchingTable";
import Layout from "../layouts/Layout.astro";

const BASE_URL = "http://localhost:4321";

const response = await fetch(`${BASE_URL}/api/vouching-data`);
const json = await response.json();
const vouchingData = json.data;
---

<Layout>
    <VouchingTable data={vouchingData} client:load />
</Layout>
